{
  "timeline":
  {
    "headline":"History Timeline",
    "type":"default",

    "date": [
      <% for entry in @entries %>
        {
          "startDate": "<%= entry.date.to_s.gsub('-', ',') %>",
          "endDate": "<%= entry.date.to_s.gsub('-', ',') %>",
          "headline": "<%= entry.title %>",
          "text":"<p>In <%= entry.location %></p><p><%= entry.body %></p><p><%= render :partial => '/entries/buttons.html.erb', locals: {entry: entry} %></p>",
          "classname": "<%= 'timeline-entry' + (entry.id).to_s %>" 

        } <%= "," unless entry == @entries.last %>
      <% end %>
    ]
  }
}